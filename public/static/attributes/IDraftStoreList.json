{
  "classId": "idm.component.idmCustomizedComponents.idraftstorelist",
  "comName": "电气起草收藏列表",
  "className": "IDraftStoreList",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "初始加载",
          "desc": "是否在组件初始化时请求接口数据",
          "bindKey": "loadDataCreated",
          "default": true,
          "dictionary": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "显示标题",
          "desc": "是否显示标题",
          "bindKey": "showTitle",
          "default": true,
          "dictionary": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "显示搜索",
          "desc": "是否显示搜索框",
          "bindKey": "showSearch",
          "default": true,
          "dictionary": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "模块标题",
          "desc": "是否显示各个模块的标题",
          "bindKey": "showTypeTitle",
          "default": true,
          "dictionary": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "收藏图标",
          "desc": "是否显示收藏图标按钮",
          "bindKey": "showStoreButton",
          "default": true,
          "dictionary": [
            {
              "label": "是",
              "value": true
            },
            {
              "label": "否",
              "value": false
            }
          ]
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "标题",
          "desc": "组件大标题",
          "bindKey": "title",
          "display": "@[showTitle]"
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "列数",
          "desc": "列表一行展示多少列",
          "bindKey": "columnNumber",
          "default": 2
        }
      ]
    },
    {
      "type": "group",
      "text": "样式设置",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "box"
        },
        {
          "type": "inlineGroup",
          "text": "宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "width",
              "width": "60px",
              "default": "100%"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "height",
              "width": "60px",
              "default": "auto"
            }
          ]
        },
        {
          "type": "group",
          "text": "背景设置",
          "desc": "点击？查看背景设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {}
            },
            {
              "type": "uploadImage",
              "layoutType": "inline",
              "text": "背景图片",
              "bindKey": "bgImgUrl",
              "placeholder": "可输入图片地址或直接上传"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "横向偏移",
              "bindKey": "positionX",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "纵向偏移",
              "bindKey": "positionY",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "背景大小",
              "bindKey": "bgSize",
              "dictionary": [
                {
                  "label": "裁切显示",
                  "value": "cover"
                },
                {
                  "label": "完全显示",
                  "value": "contain"
                },
                {
                  "label": "自定义",
                  "value": "custom"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "宽度",
              "bindKey": "bgSizeWidth",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "高度",
              "bindKey": "bgSizeHeight",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "平铺模式",
              "bindKey": "bgRepeat",
              "dictionary": [
                {
                  "label": "双向重复",
                  "value": "repeat"
                },
                {
                  "label": "水平重复",
                  "value": "repeat-x"
                },
                {
                  "label": "垂直重复",
                  "value": "repeat-y"
                },
                {
                  "label": "不重复",
                  "value": "no-repeat"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "inline",
              "text": "背景模式",
              "bindKey": "bgAttachment",
              "dictionary": [
                {
                  "label": "固定",
                  "value": "fixed"
                },
                {
                  "label": "滚动",
                  "value": "scroll"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            }
          ]
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "border"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "文字",
          "bindKey": "font"
        }
      ]
    },
    {
      "type": "group",
      "text": "数据源",
      "children": [
        {
          "type": "dataSourceSelect",
          "layoutType": "block",
          "text": "数据源",
          "desc": "返回数据格式：[]",
          "bindKey": "dataSource"
        },
        {
          "type": "table",
          "text": "接收其他组件联动参数",
          "bindKey": "linkageParamList",
          "tabText": "参数[index]",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "表单标识",
              "bindKey": "msgKey",
              "default": ""
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "参数别名",
              "bindKey": "paramKey",
              "default": ""
            },
            {
              "type": "actionSelect",
              "layoutType": "block",
              "text": "参数处理函数",
              "bindKey": "paramFun",
              "desc": "收到其他组件联动参数后会根据表单标识调用这里设置的函数返回自定义的参数内容，返回格式为字符串或对象，例如：{text:'',value:''}或者直接字符串",
              "multiple": false
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "children": [
        {
          "type": "select",
          "layoutType": "inline",
          "text": "刷新KEY",
          "bindKey": "messageRefreshKey",
          "desc": "根据组件接收到的消息KEY来刷新数据，可选择多个",
          "default": "",
          "placeholder": "请选择刷新KEY",
          "multiple": true,
          "ctrlAttrObject": {
              "mode": "tags"
          },
          "dictionary": [
              {
                  "label": "info",
                  "value": "info"
              },
              {
                  "label": "inbox",
                  "value": "inbox"
              }
          ]
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "收藏自定义函数",
          "bindKey": "storeCustomerFunction",
          "desc": "点击收藏图标后触发自定义函数,参数为：{item,_this}",
          "multiple": false
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "取消收藏自定义函数",
          "bindKey": "unStoreCustomerFunction",
          "desc": "点击取消收藏图标后触发自定义函数,参数为：{item,_this}",
          "multiple": false
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "行点击事件",
          "bindKey": "itemClickCustomFunction",
          "desc": "点击行触发的自定义事件，参数为：{item,_this}",
          "multiple": false
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "搜索事件",
          "bindKey": "searchCustomFunction",
          "desc": "点击行触发的自定义事件，参数为：{value,_this}",
          "multiple": false
        }
      ]
    }
  ]
}